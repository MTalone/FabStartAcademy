<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FabStartAcademy</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/bootstrap-icons.css" />
</head>
<body>
    <div class="">
        <div class="col-2 float-left">
            <div class="card">
                <div class="card-header">

                    
                    <img src="~/imgs/thumb_3_image_big.png"  style="max-width:90%" />
                </div>
                <div class="card-body min-vh-100">Links</div>
              </div>
        
        </div>
        <div class="col-8 float-left">
           
                @RenderBody()
            
        </div>
        
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - FabStartAcademy - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
